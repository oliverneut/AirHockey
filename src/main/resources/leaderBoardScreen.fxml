   <children>
      <TableView fx:id="leaderboardTable" layoutY="100.0" prefHeight="500.0" prefWidth="400.0" />
      <Button fx:id="goBackButton" layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#goBack" prefHeight="45.0" prefWidth="45.0" text="&lt;" />
      <Button fx:id="friendsButton" layoutX="230.0" layoutY="26.0" mnemonicParsing="false" onAction="#showFriends" text="friends" />
      <Button fx:id="allButton" layoutX="82.5" layoutY="26.0" mnemonicParsing="false" onAction="#showAll" prefHeight="42.0" prefWidth="88.0" text="all" />
   </children>
</AnchorPane>
