<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddFriendsScreenController">
   <children>
      <TextField fx:id="friendsTextField" layoutX="27.0" layoutY="103.0" prefHeight="32.0" prefWidth="267.0" />
      <Label layoutX="31.0" layoutY="56.0" prefHeight="33.0" prefWidth="330.0" text="add friend by username :" />
      <Button fx:id="searchButton" layoutX="317.0" layoutY="98.0" mnemonicParsing="false" onAction="#searchFriends" prefHeight="42.0" prefWidth="51.0" text="-&gt;" />
      <TableView layoutX="27.0" layoutY="168.0" prefHeight="186.0" prefWidth="337.0">
        <columns>
          <TableColumn fx:id="usernameList" prefWidth="329.0" text="usernames" />
        </columns>
      </TableView>
      <TableView layoutX="27.0" layoutY="383.0" prefHeight="203.0" prefWidth="221.0">
        <columns>
          <TableColumn fx:id="friendList" prefWidth="219.0" text="friend requests" />
        </columns>
      </TableView>
      <Button fx:id="acceptButton" layoutX="270.0" layoutY="435.0" mnemonicParsing="false" onAction="#acceptFriendRequest" prefHeight="42.0" prefWidth="94.0" text="accept" />
      <Button fx:id="declineButton" layoutX="270.0" layoutY="505.0" mnemonicParsing="false" onAction="#declineFriendRequest" prefHeight="42.0" prefWidth="94.0" text="decline" />
      <Button fx:id="goBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="42.0" prefWidth="42.0" text="&lt;" />
   </children>
</AnchorPane>
