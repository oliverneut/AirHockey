<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddFriendsScreenController">
   <children>
      <TextField fx:id="friendsTextField" layoutX="27.0" layoutY="94.0" prefHeight="32.0" prefWidth="267.0" promptText="search" />
      <Button fx:id="searchButton" layoutX="319.0" layoutY="89.0" mnemonicParsing="false" onAction="#searchFriends" prefHeight="42.0" prefWidth="51.0" text="-&gt;" />
      <Button fx:id="acceptButton" layoutX="297.0" layoutY="398.0" mnemonicParsing="false" onAction="#acceptFriendRequest" prefHeight="42.0" prefWidth="94.0" text="accept" />
      <Button fx:id="declineButton" layoutX="297.0" layoutY="492.0" mnemonicParsing="false" onAction="#declineFriendRequest" prefHeight="42.0" prefWidth="94.0" text="decline" />
      <Button fx:id="goBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="42.0" prefWidth="42.0" text="&lt;" />
      <ListView fx:id="addFriendList" layoutX="27.0" layoutY="146.0" prefHeight="154.0" prefWidth="267.0" />
      <Button fx:id="addButton" layoutX="312.0" layoutY="258.0" mnemonicParsing="false" onAction="#addFriend" prefHeight="42.0" prefWidth="64.0" text="add" />
      <ListView fx:id="requestList" layoutX="27.0" layoutY="380.0" prefHeight="162.0" prefWidth="267.0" />
      <Label layoutX="27.0" layoutY="332.0" text="REQUESTS" />
      <Button fx:id="refreshButton" layoutX="200.0" layoutY="328.0" mnemonicParsing="false" onAction="#refreshRequests" prefHeight="42.0" prefWidth="94.0" text="refresh" />
   </children>
</AnchorPane>
